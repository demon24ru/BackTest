{
  "name": "testtaskapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "dev": "npx nodemon ./bin/www",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo:migrate": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:undo:seed": "npx sequelize-cli db:seed:undo:all",
    "db:init": "npx sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express-http-context": "^1.2.4",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "marked": "^2.0.1",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "dotenv": "^10.0.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "mocha": "^5.1.1",
    "supertest": "^3.0.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "sequelize-cli": "^6.2.0"
  }
}
